package Pruebas;
import java.util.Scanner;
public class Renos {
public static void main(String[] args) {
	int nieve;
	int i=0;
	String almoadilla="";

	Scanner usuarioreno= new Scanner(System.in);
	System.out.println("Dime longitud de pista:");
	nieve=usuarioreno.nextInt();
	

	for (i=0; i==nieve; i++) {

	almoadilla= "#";
	almoadilla= almoadilla+almoadilla;
	}
	System.out.println(almoadilla);
}
}
