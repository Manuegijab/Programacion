package boletintablas;

import Pruebas.tabla;

public class Ejercicio4 {
public static void main(String[] args) {
	
	int tabla1[]= {1,4,2,5};
	int tabla2[]= {1,4,5,2};
	
	boolean asdf= true;
	int i=0;
	
	while(!asdf) {
	 if (tabla1[i]==tabla2[i]) {
		 asdf=true;
		 i++;
		 
	 }
	 else;
	 asdf=false;
	}
	
	
}
}
